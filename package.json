{
  "name": "soshPCShare",
  "version": "1.1.0",
  "description": "Social Share For Desktop",
  "author": {
    "name": "Mich",
    "email": "mukuashi@xiaomi.com",
    "url": "http://www.miui.com"
  },

  "repository": "miui/soshPCShare",
  "license": "MIT",
  "main": "dist/sosh.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.config.js",
    "js": "cross-env NODE_ENV=production webpack",
    "minify": "uglifyjs --mangle --compress -o ./dist/sosh.min.js ./dist/sosh.js",
    "build": "npm run js && npm run minify",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --inline --hot",
    "build": "webpack --display-error-details",
    "watch": "webpack --progress --colors --watch"

  },
  "keywords": [
    "share",
    "socialshare",
    "weixin",
    "qq",
    "weibo",
    "qzone",
    "qrcode"
  ],
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "easy-classlist": "^0.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "shim4webpack": "^0.0.2",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "xtend": "^4.0.1"
  },
  "dependencies": {
    "easy-classlist": "0.0.1",
    "xtend": "^4.0.1"
  }
}
